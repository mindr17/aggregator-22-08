{
  "name": "aggregator",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npx ts-node-dev --respawn --transpile-only ./src/index.ts",
    "build": "esbuild ./src/index.ts --bundle --outfile=bundle.js --platform=node --format=esm --external:pg-native --external:aws-sdk --external:mock-aws-s3 --external:nock",
    "mytest": "npm run build && node bundle.js",
    "start": "node ./src/auth/app.js",
    "server": "nodemon ./src/auth/app.js"
  },
  "license": "MIT",
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.9",
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.48",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.4",
    "node-pre-gyp": "^0.17.0",
    "pg": "^8.8.0",
    "typescript": "^4.7.4",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.13",
    "@types/pg": "^8.6.5",
    "@types/ws": "^8.5.3",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1"
  }
}
